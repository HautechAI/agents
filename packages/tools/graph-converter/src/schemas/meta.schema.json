{
  "$id": "meta.schema.json",
  "type": "object",
  "required": ["name", "version", "updatedAt", "format"],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "version": {
      "type": "integer",
      "minimum": 0
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    },
    "format": {
      "type": "integer",
      "const": 2
    }
  }
}
